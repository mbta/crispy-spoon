<%= for headsign <- @time_data do %>
  <%= for %{prediction: %{time: time}} <- headsign.times do %>
    <div class="route-branch-stop-prediction">
      <span class="route-branch-headsign-wrapper">
        <span class="route-branch-headsign"><%= headsign.name %></span>
      </span>
      <strong><%= time %></strong>
    </div>
  <% end %>  
<% end %>