<div class="m-event-listing">
  <nav class="m-event-list__nav--mobile-controls sticky-top">
    <%= render "_month_select.html",
        conn: @conn,
        month: @month,
        year: @year %>

    <%= render "_year_select.html",
        conn: @conn,
        month: @month,
        year: @year %>
  </nav>

  <%= render "_list_of_events.html",
      conn: @conn,
      events: @events,
      month: @month,
      year: @year %>

</div>
