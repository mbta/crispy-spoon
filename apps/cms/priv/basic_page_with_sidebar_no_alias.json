      "field_custom_html_body": [
        {
          "value": "<h2>Hazardous Waste Management</h2>\r\n\r\n<ul>\r\n\t<li>Waste oil is recycled and sold to save money and reduce environmental impacts.</li>\r\n\t<li>Leftover aerosol in cans is released into 55-gallon drums for proper disposal, and the cans are recycled.</li>\r\n\t<li>All freon from air conditioners is captured and reused.</li>\r\n\t<li>Batteries are returned to manufacturers to ensure proper recycling.</li>\r\n\t<li>All bus tires are leased from the manufacturers, and returned to be recycled after a certain number of miles</li>\r\n</ul>\r\n",
          "format": "basic_html",
          "processed": "<h2>Hazardous Waste Management</h2>\n\n<ul><li>Waste oil is recycled and sold to save money and reduce environmental impacts.</li>\n\t<li>Leftover aerosol in cans is released into 55-gallon drums for proper disposal, and the cans are recycled.</li>\n\t<li>All freon from air conditioners is captured and reused.</li>\n\t<li>Batteries are returned to manufacturers to ensure proper recycling.</li>\n\t<li>All bus tires are leased from the manufacturers, and returned to be recycled after a certain number of miles</li>\n</ul>"
        }
      ]
    }
